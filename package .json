      "mathjax-full": "3.2.2",
      "nano-staged": "0.8.0",
      "nodejs-jieba": "0.0.2",
      "prettier": "3.0.3",
      "vue": "3.3.7",
      "vuepress": "2.0.0-beta.67",
      "vuepress-theme-hope": "2.0.0-beta.241"
